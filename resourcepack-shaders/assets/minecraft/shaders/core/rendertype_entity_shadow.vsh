#version 330
#define VSH

#moj_import <utils.glsl>

in vec3 Position;
in vec4 Color;
in vec2 UV0;

uniform mat4 ModelViewMat;
uniform mat4 ProjMat;

out vec4 vertexColor;
out vec2 texCoord0;
out vec4 glpos;

void main() {
    gl_Position = VSH_DISCARD; //ProjMat * ModelViewMat * vec4(Position, 1.0);

    vertexColor = Color;
    texCoord0 = UV0;
    glpos = gl_Position;
}
